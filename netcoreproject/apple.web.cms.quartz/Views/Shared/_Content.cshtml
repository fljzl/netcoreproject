<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta name="renderer" content="webkit">
    <title>@ViewBag.title</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <link rel="stylesheet" href="~/Content/Dist/layui/css/layui.css" type="text/css">
    <link rel="stylesheet" href="~/Content/Dist/layui/font-awesome/css/iconfont.css" type="text/css">
    <link rel="stylesheet" href="~/Content/Css/main.css?" type="text/css">

    @RenderSection("styles", required: false)
</head>
<body>
    @RenderBody()
    <script src="~/Content/Js/jquery-1.8.3.js" type="text/javascript"></script>
    <script src="~/Content/Dist/layui/layui.js" type="text/javascript"></script>
    <script src="~/Content/Js/common.js?v=2020" type="text/javascript"></script>

    @RenderSection("scripts", required: false)
    <script type="text/javascript">
        window.onload = function () {
            addEventListener('message', function (e) { });
        }
    </script>
</body>
</html>